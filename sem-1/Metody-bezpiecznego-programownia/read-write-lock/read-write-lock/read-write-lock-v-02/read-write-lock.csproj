<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework> <!-- Or net8.0, etc. -->
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <!-- RootNamespace typically defaults to the project folder name (MyLockTestApp) -->
    <RootNamespace>MyLockTestApp</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <!--
      If CustomLocks is a separate project (as defined above) and you want to reference it:
      Adjust the path to your CustomLocks.csproj file.
    -->
    <!--
    <ProjectReference Include="..\CustomLocks\CustomLocks.csproj" />
    -->

    <!--
      Alternatively, if your lock .cs files (IReadWriteLock.cs, etc.) are directly
      within this project's folder (e.g., C:\Users\tomka\repos\Masters\sem-1\Metody-bezpiecznego-programownia\read-write-lock\read-write-lock\read-write-lock-v-02\),
      they will be compiled automatically into this executable, and you
      wouldn't need the ProjectReference. This is the assumed setup.
    -->
  </ItemGroup>

</Project>
